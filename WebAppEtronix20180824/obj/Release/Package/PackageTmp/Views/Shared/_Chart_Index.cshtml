    //date validation , it must be set otherwhise, is error
    function isValidDate(s) {
        var bits = s.split('-');

        var currentDate = new Date(s);

        var sYear = parseInt(bits[0]);
        var sMonth = parseInt(bits[1]);
        var sDay = parseInt(bits[2]);

        // 2011 is presented as 111
        var vYear = currentDate.getYear() + 1900;
        // Jan =0
        var vMonth = currentDate.getMonth() + 1;
        var vDay = currentDate.getDate();

        if (sYear === vYear &&
            sMonth === vMonth &&
            sDay === vDay) {
            return true;

        }
        return false;
    }

