    function CheckLeapYear(pYear) {
        if (pYear === null) {
            console.log("Year is not set ");
            return false;
        }
        if ((pYear % 4 == 0) && (pYear % 100 != 0) || (pYear % 400 == 0))
        {
            console.log("Year is a leap one");
            return true;
        }
        console.log("Year is not a leap one");
        return false;
    }



    
