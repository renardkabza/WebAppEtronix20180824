                pointFormat: '<tr><td style="color:{series.color};padding:0">{series.name}: </td>' + '<td style="padding:0"><b>{point.y:.2f}</b></td>' + '</tr>',
                footerFormat: '</table>',
                shared: true,
                useHTML: true,

                positioner: function (boxWidth, boxHeight, point) {
                    return { x: point.plotX, y: point.plotY - 50 };
                }
                
            },
            yAxis: seriesYaxis,
                

            rangeSelector: {
                inputEnabled: false, //disable range selectors
                selected: 7, //0 1st button
                buttons: [
                    {
                        type: 'minute',
                        count: 5,
                        text: '5M'
                    },
                    {
                        type: 'hour',
                        count: 1,
                        text: '1h'
                    },
                    {
                        type: 'day',
                        count: 1,
                        text: '1d'
                    },

                    {
                        type: 'week',
                        count: 1,
                        text: '1w'
                    },
                    {
                        type: 'month',
                        count: 1,
                        text: '1m'
                    },
                    {
                        type: 'year',
                        count: 1,
                        text: '1y'
                    },
                    {
                        type: 'ytd',
                        text: 'YTD'
                    },
                    {
                        type: 'all',
                        text: 'All'
                    }
                ]
            },
            plotOptions: {
                series: {
                    states: {
                        hover: {
                            enabled: false
                        }
                    }
                }
            }
        };
        Highcharts.stockChart('ChartContainer', options);

    }

    function OpenChartControlPanelBody() {
        $('#ChartControlPanelBody').show();
    }

    function HideChartControlPanelBody() {
        $('#ChartControlPanelBody').hide();
    }
    

</script>

